{
    "name": "eternal-empire",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "test": "node test/runTests.js",
        "test:studio": "node test/runTests.js --mode=studio",
        "lint": "npx eslint src --no-error-on-unmatched-pattern",
        "asset-sync": "npx rbxts-asset-sync --output=src/shared/asset/AssetMap.ts",
        "asset-watch": "npx rbxts-asset-sync --watch --output=src/shared/asset/AssetMap.ts",
        "build": "npm run asset-sync & rbxtsc",
        "plugin": "cd ./plugin && npx rojo build --plugin ee-tooling.rbxmx",
        "docs": "npx typedoc",
        "dev": "concurrently --prefix none \"rbxtsc -w --optimizedLoops\" \"npx rojo serve\"",
        "plugin:test": "node --test plugin/datamodel/*.test.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/evilbocchi/eternal-empire.git"
    },
    "bugs": {
        "url": "https://github.com/evilbocchi/eternal-empire/issues"
    },
    "homepage": "https://github.com/evilbocchi/eternal-empire#readme",
    "devDependencies": {
        "@eslint/js": "^9.34.0",
        "@modelcontextprotocol/sdk": "^1.19.1",
        "@rbxts/compiler-types": "^3.0.0-types.0",
        "@rbxts/types": "^1.0.885",
        "@types/node": "^24.3.0",
        "@typescript-eslint/eslint-plugin": "^8.40.0",
        "@typescript-eslint/parser": "^8.40.0",
        "axios": "^1.12.2",
        "concurrently": "^9.2.1",
        "dotenv": "^17.2.3",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-no-relative-import-paths": "^1.6.1",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-roblox-ts": "^1.1.0",
        "express": "^5.1.0",
        "prettier": "^3.6.2",
        "rbxts-asset-sync": "^0.3.3",
        "rbxts-transform-env": "^3.0.0",
        "rbxts-transformer-flamework": "^1.3.2",
        "rbxts-transformer-services": "^1.1.1",
        "roblox-ts": "^3.0.0",
        "rojo": "^1.0.2",
        "rokit-npm": "^1.0.1",
        "signale": "^1.4.0",
        "tsx": "^4.20.6",
        "typedoc": "^0.28.13",
        "typedoc-material-theme": "^1.4.0",
        "typedoc-plugin-extras": "^4.0.1",
        "typedoc-plugin-missing-exports": "^4.1.2",
        "typedoc-plugin-rename-defaults": "^0.7.3",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.40.0",
        "ws": "^8.18.3"
    },
    "dependencies": {
        "@antivivi/lemon-signal": "^1.2.4",
        "@antivivi/profileservice": "^1.5.2",
        "@antivivi/rbxnamecolor": "^1.0.1",
        "@antivivi/unnamespaced-keyed": "^1.0.5",
        "@antivivi/vrldk": "^1.1.0",
        "@flamework/components": "^1.3.2",
        "@flamework/core": "^1.3.1",
        "@rbxts/camera-shaker": "^1.0.8",
        "@rbxts/chat-service": "^1.3.0",
        "@rbxts/ejt": "^2.0.5",
        "@rbxts/flamework-binary-serializer": "^0.7.0",
        "@rbxts/fletchette": "^4.5.0",
