{
    "name": "eternal-empire",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "test": "npm run build --silent && npm run test:cloud --silent",
        "test:studio": "node test/runTests.js --mode=studio",
        "test:cloud": "node sandbox/test.js",
        "lint": "npx eslint src --no-error-on-unmatched-pattern",
        "asset-sync": "npx rbxts-asset-sync --log-level=warn --output=src/shared/asset/AssetMap.ts",
        "asset-watch": "npx rbxts-asset-sync --watch --output=src/shared/asset/AssetMap.ts",
        "build:src": "npm run asset-sync && rbxtsc",
        "build:place": "npx rojo build --output=sandbox/local.rbxl && node sandbox/pull.js && npx lune run sandbox/inject.luau",
        "build": "npm run build:src && npm run build:place",
        "open": "node sandbox/open.js",
        "plugin": "cd ./plugin && npx rojo build --plugin ee-tooling.rbxmx",
        "plugin:test": "node --test plugin/datamodel/*.test.js",
        "docs": "npx typedoc && node test/runProgressionEstimate.js",
        "dev": "concurrently --prefix none \"rbxtsc -w --optimizedLoops\" \"npx rojo serve\"",
        "dev:test": "npm run build:place --silent && npm run test:cloud --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/evilbocchi/eternal-empire.git"
    },
    "bugs": {
        "url": "https://github.com/evilbocchi/eternal-empire/issues"
    },
    "homepage": "https://github.com/evilbocchi/eternal-empire#readme",
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@modelcontextprotocol/sdk": "^1.23.0",
        "@rbxts/compiler-types": "^3.0.0-types.0",
        "@rbxts/types": "^1.0.894",
        "@types/node": "^24.10.1",
        "axios": "^1.13.2",
        "concurrently": "^9.2.1",
        "dotenv": "^17.2.3",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-no-relative-import-paths": "^1.6.1",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-roblox-ts": "^1.3.0",
        "express": "^5.1.0",
        "prettier": "^3.6.2",
        "rbx-lune": "^1.0.0",
        "rbxts-asset-sync": "^0.4.2",
        "rbxts-transform-env": "^3.0.0",
        "rbxts-transformer-flamework": "^1.3.2",
        "rbxts-transformer-services": "^1.1.1",
        "roblox-ts": "^3.0.0",
        "rojo": "^1.0.3-rojo7.6.1",
        "signale": "^1.4.0",
        "typedoc": "^0.28.14",
        "typedoc-material-theme": "^1.4.1",
        "typedoc-plugin-extras": "^4.0.1",
        "typedoc-plugin-missing-exports": "^4.1.2",
        "typedoc-plugin-rename-defaults": "^0.7.3",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.47.0",
        "ws": "^8.18.3"
    },
    "dependencies": {
        "@antivivi/lemon-signal": "^1.2.4",
        "@antivivi/profileservice": "^1.5.2",
        "@antivivi/rbxnamecolor": "^1.0.1",
        "@antivivi/vrldk": "^1.1.0",
        "@flamework/components": "^1.3.2",
        "@flamework/core": "^1.3.2",
        "@rbxts/camera-shaker": "^1.0.8",
        "@rbxts/ejt": "^2.0.5",
        "@rbxts/flamework-binary-serializer": "^0.7.0",
        "@rbxts/fletchette": "^4.5.0",
        "@rbxts/fuzzy-search": "^1.0.4",
        "@rbxts/janitor": "^1.18.3-ts.0",
        "@rbxts/jest": "^3.13.3-alpha.2",
        "@rbxts/jest-globals": "^3.13.3-alpha.2",
        "@rbxts/partcache": "^1.1.1",
        "@rbxts/react": "^17.3.7-alpha.1",
        "@rbxts/react-devtools-core": "^17.3.7-alpha.1",
        "@rbxts/react-globals": "^17.3.7-alpha.1",
        "@rbxts/react-roblox": "^17.3.7-alpha.1",
        "@rbxts/roman-numerals": "^1.0.2",
        "@rbxts/rparticle": "^1.0.0",
        "@rbxts/serikanum": "^1.6.0",
        "@rbxts/services": "^1.6.0",
        "@rbxts/stringbuilder": "^1.1.0",
        "@rbxts/ui-labs": "^2.4.2"
    }
}
