{
    "$schema": "https://typedoc.org/schema.json",
    "plugin": [
        "typedoc-material-theme",
        "typedoc-plugin-missing-exports",
        "typedoc-plugin-extras",
        "typedoc-plugin-rename-defaults"
    ],
    "projectDocuments": [
        "docs/*.md"
    ],
    "placeInternalsInOwningModule": true,
    "highlightLanguages": [
        "lua",
        "luau",
        "typescript",
        "tsx",
        "jsx",
        "sh"
    ],
    "entryPoints": [
        "src"
    ],
    "exclude": [
        "**/node_modules/**",
        "src/server/quests/**/*",
        "src/shared/items/**/*",
    ],
    "out": "./docsout",
    "skipErrorChecking": true,
    "excludeExternals": true,
    "suppressCommentWarningsInDeclarationFiles": true,
    "validation": {
        "notExported": false,
        "invalidLink": false,
        "notDocumented": false
    },
    "blockTags": [
        "@fileoverview",
        "@borrows",
        "@since",
        "@return",
        "@public",
        "@private",
        "@remarks",
        "@module",
        "@moduleoverview",
        "@packageDocumentation",
        "@param",
        "@returns",
        "@template",
        "@typeParam",
        "@see",
        "@link",
        "@deprecated",
        "@example",
        "@throws",
        "@defaultValue",
        "@event",
        "@inheritDoc",
        "@internal",
        "@override",
        "@property",
        "@virtual"
    ],

    "favicon": "assets/brand/Cover.png",
    
    "customTitle": "eternal empire",
    "customDescription": "Build your money-making empire. Uncover the world of Obbysia.",
    "entryPointStrategy": "expand",
    "themeColor": "#E8DDE1",
}